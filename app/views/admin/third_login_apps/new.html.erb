<%= form_with model: [:admin, @third_login_app], class: "form" do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control", placeholder: "第三方英文简写" %>
  </div>

  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_field :description, class: "form-control", placeholder: "备注" %>
  </div>


    <div class="form-group">
    <%= f.label :login_url %>
    <%= f.text_field :login_url, class: "form-control", placeholder: "备注" %>
  </div>

  <div class="form-group">
    <%= f.submit "提交" %>
  </div>


<% end %>